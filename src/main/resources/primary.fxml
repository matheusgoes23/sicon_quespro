<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="720.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ufersa.sicon_quespro.controller.PrimaryController">
    <children>
        <Label text="Primary View" />
        <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Switch to Secondary View" />

        <TextField id="txtFullName" fx:id="txtFullName" promptText="Enter your name">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
